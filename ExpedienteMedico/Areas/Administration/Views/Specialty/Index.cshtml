
@model IEnumerable<Specialty>


<partial name="_Notifications"></partial> <!--Toaster notifications-->

<div class="container p-3"> 
    <div class="row pt-4"> 
        <div class="col-6">
            <h2 class="text-decoration">Specialty List</h2>
        </div>

        <div class="col-6 text-end p-1">
            <a asp-controller="Specialty" asp-action="Create" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i>
                Create new specialty
            </a>
        </div>

    </div>
    <br>    

    <table id="tblDataSpe" class ="table table-bordered" style="width:100%">
        <thead>
        <tr>
            <th>Specialty Name</th>
            <th>Actions</th>
        </tr>
        </thead>
    </table>

</div>


@section Scripts{
    <script src="~/js/jsSpecialty.js"></script>
}